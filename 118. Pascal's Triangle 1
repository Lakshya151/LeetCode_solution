class Solution {
public:
    vector<vector<int>> generate(int numRows) {
        vector<vector<int>>ans;
        vector<int>arr(numRows+1,0);
        arr[0]=1;
        ans.push_back(vector<int>(arr.begin(),arr.begin()+1));
        for(int i=1;i< numRows;i++){
            for(int j=i;j>0;j--){
                arr[j]=arr[j]+arr[j-1];
            }
            ans.push_back(vector<int>(arr.begin(),arr.begin()+i+1)); //here I dont push whole vector , as there are 0's in there and i want only non zeros values
        }
        return ans;
    }
};
