class Solution {
public:
    int majorityElement(vector<int>& nums) {
        int n=nums.size();
        unordered_map<int,int>s; //Use map , so that on a value i can just increase its repeation count
        for(int x:nums){
            s[x]++;
            if(s[x]>n/2){
                return x;
            }
        }
        return -1;
    }
};
